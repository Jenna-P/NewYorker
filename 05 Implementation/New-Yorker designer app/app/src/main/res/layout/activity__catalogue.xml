                android:contentDescription="@string/_47_dkr"
                app:srcCompat="@drawable/catalougeimage" />

            <ImageView
                android:id="@+id/wallchoice8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/_47_dkr"
                app:srcCompat="@drawable/catalougeimage" />

            <ImageView
                android:id="@+id/wallchoice9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/_47_dkr"
                app:srcCompat="@drawable/catalougeimage" />
